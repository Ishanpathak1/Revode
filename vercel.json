{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/health",
        "dest": "/server.js"
      },
      {
        "src": "/test-openai",
        "dest": "/server.js"
      },
      {
        "src": "/((?!api).*)",
        "dest": "/build/index.html"
      }
    ]
  }