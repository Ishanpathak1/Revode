{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node@18"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "build/$1"
      }
    ]
  }